{
    "name": "jaedm97/monsterdevs-tools",
    "description": "Helper Functions for MonsterDevs Tools",
    "type": "library",
    "version": "1.0.0",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Jaed Mosharraf",
            "email": "jaedm97@gmail.com"
        }
    ],
    "minimum-stability": "stable",
	"prefer-stable": true,
    "require": {
        "php": ">= 8.0",
        "wp-cli/wp-config-transformer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "MonsterDevs\\Tools\\": "src/"
        }
    },
    "scripts": {
        "build": [
			"composer dump-autoload"
		],
        "build-nd": [
			"composer dump-autoload --no-dev"
		]
	},
    "config": {
		"optimize-autoloader": true
	}
}
